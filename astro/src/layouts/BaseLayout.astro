---
import BaseHead from '../components/BaseHead.astro';
import Nav from '../components/Nav.astro';

const {title, description, permalink, publishDate, ogImageUrl, canonicalURL } = Astro.props;
---
<html lang="en">
  <head>
    <BaseHead title={title} description={description} permalink={permalink} canonicalURL={canonicalURL} publishDate={publishDate} ogImageUrl={ogImageUrl} />
    <!--<link rel="stylesheet" href={Astro.resolve("../assets/global.css")} />-->
    <style global>
			@import "../assets/global.css";
  	</style>

  </head>
  <body>
  <Nav/>
  <slot/>
  </body>
</html>
